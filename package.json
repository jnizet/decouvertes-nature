{
  "name": "decouvertes-nature",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "NG_BUILD_OPTIMIZE_CHUNKS=1 ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "format": "prettier --write \"src/**/*.+(ts|js|json|html|scss)\"",
    "format:check": "prettier --check \"src/**/*.+(ts|js|json|html|scss)\"",
    "serve:e2e": "ng serve -c e2e",
    "e2e": "yarn playwright test",
    "build:e2e:ci": "ng build -c e2e",
    "serve:e2e:ci": "serve dist/decouvertes-nature/browser --single --listen 4201",
    "e2e:ci": "concurrently --names \"emulators,app,test\" --success first --kill-others \"firebase emulators:start --import=emulator-data\" \"yarn build:e2e:ci && yarn serve:e2e:ci\" \"wait-on --timeout 180000 http://localhost:4201 http://localhost:4000 && yarn playwright test\""
  },
  "private": true,
  "engines": {
    "node": "20",
    "yarn": ">=1.22.10 <2.0.0"
  },
  "dependencies": {
    "@angular/animations": "20.0.4",
    "@angular/common": "20.0.4",
    "@angular/compiler": "20.0.4",
    "@angular/core": "20.0.4",
    "@angular/fire": "20.0.1",
    "@angular/forms": "20.0.4",
    "@angular/platform-browser": "20.0.4",
    "@angular/router": "20.0.4",
    "@ng-bootstrap/ng-bootstrap": "19.0.0-rc.0",
    "@popperjs/core": "2.11.8",
    "bootstrap": "5.3.6",
    "bootstrap-icons": "1.13.1",
    "date-fns": "4.1.0",
    "firebase": "11.9.1",
    "leaflet": "1.9.4",
    "ngx-valdemort": "13.0.0",
    "rxjs": "7.8.2",
    "tslib": "2.8.1",
    "zone.js": "0.15.1"
  },
  "devDependencies": {
    "@angular/build": "20.0.3",
    "@angular/cli": "20.0.3",
    "@angular/compiler-cli": "20.0.4",
    "@angular/localize": "20.0.4",
    "@axe-core/playwright": "4.10.2",
    "@eslint/js": "9.28.0",
    "@playwright/test": "1.52.0",
    "@types/jasmine": "5.1.8",
    "@types/leaflet": "1.9.18",
    "angular-eslint": "19.8.1",
    "concurrently": "9.1.2",
    "eslint": "9.28.0",
    "eslint-config-prettier": "10.1.5",
    "eslint-plugin-prettier": "5.4.1",
    "firebase-admin": "13.4.0",
    "firebase-tools": "14.8.0",
    "globals": "16.2.0",
    "jasmine-core": "5.8.0",
    "karma": "6.4.4",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage": "2.2.1",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.1.0",
    "prettier": "3.5.3",
    "serve": "14.2.4",
    "typescript": "5.8.3",
    "typescript-eslint": "8.33.1",
    "wait-on": "8.0.3"
  },
  "resolutions": {
    "firebase": "11.9.1"
  },
  "type": "module",
  "packageManager": "yarn@1.22.22"
}
