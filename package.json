{
  "name": "decouvertes-nature",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "NG_BUILD_OPTIMIZE_CHUNKS=1 ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "format": "prettier --write \"src/**/*.+(ts|js|json|html|scss)\"",
    "format:check": "prettier --check \"src/**/*.+(ts|js|json|html|scss)\"",
    "serve:e2e": "ng serve -c e2e",
    "e2e": "yarn playwright test",
    "build:e2e:ci": "ng build -c e2e",
    "serve:e2e:ci": "serve dist/decouvertes-nature/browser --single --listen 4201",
    "e2e:ci": "concurrently --names \"emulators,app,test\" --success first --kill-others \"firebase emulators:start --import=emulator-data\" \"yarn build:e2e:ci && yarn serve:e2e:ci\" \"wait-on --timeout 180000 http://localhost:4201 http://localhost:4000 && yarn playwright test\""
  },
  "private": true,
  "engines": {
    "node": "20",
    "yarn": ">=1.22.10 <2.0.0"
  },
  "dependencies": {
    "@angular/animations": "19.2.3",
    "@angular/common": "19.2.3",
    "@angular/compiler": "19.2.3",
    "@angular/core": "19.2.3",
    "@angular/fire": "19.0.0",
    "@angular/forms": "19.2.3",
    "@angular/platform-browser": "19.2.3",
    "@angular/platform-browser-dynamic": "19.2.3",
    "@angular/router": "19.2.3",
    "@ng-bootstrap/ng-bootstrap": "18.0.0",
    "@popperjs/core": "2.11.8",
    "bootstrap": "5.3.3",
    "bootstrap-icons": "1.11.3",
    "date-fns": "4.1.0",
    "firebase": "11.5.0",
    "leaflet": "1.9.4",
    "ngx-valdemort": "13.0.0",
    "rxjs": "7.8.2",
    "tslib": "2.8.1",
    "zone.js": "0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "19.2.5",
    "@angular/cli": "19.2.5",
    "@angular/compiler-cli": "19.2.3",
    "@angular/localize": "19.2.3",
    "@axe-core/playwright": "4.10.1",
    "@eslint/js": "9.23.0",
    "@playwright/test": "1.51.1",
    "@types/jasmine": "5.1.7",
    "@types/leaflet": "1.9.17",
    "angular-eslint": "19.3.0",
    "concurrently": "9.1.2",
    "eslint": "9.23.0",
    "eslint-config-prettier": "10.1.1",
    "eslint-plugin-prettier": "5.2.5",
    "firebase-admin": "13.2.0",
    "firebase-tools": "13.35.1",
    "globals": "16.0.0",
    "jasmine-core": "5.6.0",
    "karma": "6.4.4",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage": "2.2.1",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.1.0",
    "prettier": "3.5.3",
    "serve": "14.2.4",
    "typescript": "5.8.2",
    "typescript-eslint": "8.27.0",
    "wait-on": "8.0.3"
  },
  "resolutions": {
    "firebase": "11.5.0"
  },
  "type": "module"
}
