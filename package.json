{
  "name": "decouvertes-nature",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "format": "prettier --write \"src/**/*.+(ts|js|json|html|scss)\"",
    "format:check": "prettier --check \"src/**/*.+(ts|js|json|html|scss)\"",
    "postinstall": "node generate-bootstrap-icons.mjs",
    "serve:e2e": "ng serve -c e2e",
    "e2e": "yarn playwright test",
    "build:e2e:ci": "ng build -c e2e",
    "serve:e2e:ci": "serve dist/decouvertes-nature --single --listen 4201",
    "e2e:ci": "concurrently --names \"emulators,app,test\" --success first --kill-others \"firebase emulators:start --import=emulator-data\" \"yarn build:e2e:ci && yarn serve:e2e:ci\" \"wait-on --timeout 180000 http://localhost:4201 http://localhost:4000 && yarn playwright test\""
  },
  "private": true,
  "engines": {
    "node": ">=16.15.0",
    "yarn": ">=1.22.10 <2.0.0"
  },
  "dependencies": {
    "@angular/animations": "14.2.9",
    "@angular/common": "14.2.9",
    "@angular/compiler": "14.2.9",
    "@angular/core": "14.2.9",
    "@angular/fire": "7.4.1",
    "@angular/forms": "14.2.9",
    "@angular/platform-browser": "14.2.9",
    "@angular/platform-browser-dynamic": "14.2.9",
    "@angular/router": "14.2.9",
    "@ng-bootstrap/ng-bootstrap": "13.1.0",
    "@popperjs/core": "2.11.6",
    "bootstrap": "5.2.2",
    "bootstrap-icons": "1.9.1",
    "date-fns": "2.29.3",
    "firebase": "9.13.0",
    "leaflet": "1.9.2",
    "ngx-valdemort": "8.1.0",
    "rxjs": "7.5.7",
    "tslib": "2.4.1",
    "zone.js": "0.11.8"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "14.2.8",
    "@angular-eslint/builder": "14.1.2",
    "@angular-eslint/eslint-plugin": "14.1.2",
    "@angular-eslint/eslint-plugin-template": "14.1.2",
    "@angular-eslint/template-parser": "14.1.2",
    "@angular/cli": "14.2.8",
    "@angular/compiler-cli": "14.2.9",
    "@angular/localize": "14.2.9",
    "@axe-core/playwright": "4.5.0",
    "@playwright/test": "1.27.1",
    "@types/jasmine": "4.3.0",
    "@types/leaflet": "1.9.0",
    "@typescript-eslint/eslint-plugin": "5.42.0",
    "@typescript-eslint/parser": "5.42.0",
    "concurrently": "7.5.0",
    "eslint": "8.26.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "firebase-tools": "11.16.0",
    "jasmine-core": "4.5.0",
    "karma": "6.4.1",
    "karma-chrome-launcher": "3.1.1",
    "karma-coverage": "2.2.0",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.0.0",
    "prettier": "2.7.1",
    "serve": "14.0.1",
    "typescript": "4.8.4",
    "wait-on": "6.0.1"
  }
}
