{
  "name": "decouvertes-nature",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:e2e": "ng build -c e2e",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "format": "prettier --write \"src/**/*.+(ts|js|json|html|scss)\"",
    "format:check": "prettier --check \"src/**/*.+(ts|js|json|html|scss)\"",
    "postinstall": "node generate-bootstrap-icons.mjs",
    "serve:e2e": "serve dist/decouvertes-nature --single --listen 4201",
    "e2e": "concurrently --names \"app,test\" --success first --kill-others \"yarn serve:e2e\" \"wait-on --timeout 180000 http://localhost:4201 http://localhost:4000 && yarn playwright test\"",
    "e2e:ci": "concurrently --names \"emulators,app,test\" --success first --kill-others \"firebase emulators:start --import=emulator-data\" \"yarn build:e2e && yarn serve:e2e\" \"wait-on --timeout 180000 http://localhost:4201 http://localhost:4000 && yarn playwright test\""
  },
  "private": true,
  "engines": {
    "node": ">=14.17",
    "yarn": ">=1.22.10 <2.0.0"
  },
  "dependencies": {
    "@angular/animations": "13.2.3",
    "@angular/common": "13.2.3",
    "@angular/compiler": "13.2.3",
    "@angular/core": "13.2.3",
    "@angular/fire": "7.2.1",
    "@angular/forms": "13.2.3",
    "@angular/platform-browser": "13.2.3",
    "@angular/platform-browser-dynamic": "13.2.3",
    "@angular/router": "13.2.3",
    "@ng-bootstrap/ng-bootstrap": "12.0.0",
    "@popperjs/core": "2.11.2",
    "bootstrap": "5.1.3",
    "bootstrap-icons": "1.8.1",
    "date-fns": "2.28.0",
    "firebase": "9.6.7",
    "ngx-valdemort": "7.0.0",
    "rxfire": "6.0.3",
    "rxjs": "7.5.4",
    "tslib": "2.3.1",
    "zone.js": "0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "13.2.5",
    "@angular-eslint/builder": "13.1.0",
    "@angular-eslint/eslint-plugin": "13.1.0",
    "@angular-eslint/eslint-plugin-template": "13.1.0",
    "@angular-eslint/template-parser": "13.1.0",
    "@angular/cli": "13.2.5",
    "@angular/compiler-cli": "13.2.3",
    "@angular/localize": "13.2.3",
    "@playwright/test": "1.19.1",
    "@types/jasmine": "3.10.3",
    "@typescript-eslint/eslint-plugin": "5.12.1",
    "@typescript-eslint/parser": "5.12.1",
    "concurrently": "7.0.0",
    "eslint": "8.9.0",
    "eslint-config-prettier": "8.4.0",
    "eslint-plugin-prettier": "4.0.0",
    "firebase-tools": "10.2.1",
    "jasmine-core": "4.0.1",
    "karma": "6.3.16",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage": "2.2.0",
    "karma-jasmine": "4.0.1",
    "karma-jasmine-html-reporter": "1.7.0",
    "prettier": "2.5.1",
    "serve": "13.0.2",
    "typescript": "4.5.5",
    "wait-on": "6.0.1"
  }
}
