{
  "name": "decouvertes-nature",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "format": "prettier --write \"src/**/*.+(ts|js|json|html|scss)\"",
    "format:check": "prettier --check \"src/**/*.+(ts|js|json|html|scss)\"",
    "postinstall": "node generate-bootstrap-icons.mjs",
    "serve:e2e": "ng serve -c e2e",
    "e2e": "yarn playwright test",
    "build:e2e:ci": "ng build -c e2e",
    "serve:e2e:ci": "serve dist/decouvertes-nature --single --listen 4201",
    "e2e:ci": "concurrently --names \"emulators,app,test\" --success first --kill-others \"firebase emulators:start --import=emulator-data\" \"yarn build:e2e:ci && yarn serve:e2e:ci\" \"wait-on --timeout 180000 http://localhost:4201 http://localhost:4000 && yarn playwright test\""
  },
  "private": true,
  "engines": {
    "node": ">=16.15.0",
    "yarn": ">=1.22.10 <2.0.0"
  },
  "dependencies": {
    "@angular/animations": "15.2.2",
    "@angular/common": "15.2.2",
    "@angular/compiler": "15.2.2",
    "@angular/core": "15.2.2",
    "@angular/fire": "7.5.0",
    "@angular/forms": "15.2.2",
    "@angular/platform-browser": "15.2.2",
    "@angular/platform-browser-dynamic": "15.2.2",
    "@angular/router": "15.2.2",
    "@ng-bootstrap/ng-bootstrap": "14.0.1",
    "@popperjs/core": "2.11.6",
    "bootstrap": "5.2.3",
    "bootstrap-icons": "1.10.3",
    "date-fns": "2.29.3",
    "firebase": "9.17.2",
    "leaflet": "1.9.3",
    "ngx-valdemort": "9.0.0",
    "rxjs": "7.8.0",
    "tslib": "2.5.0",
    "zone.js": "0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "15.2.4",
    "@angular-eslint/builder": "15.2.1",
    "@angular-eslint/eslint-plugin": "15.2.1",
    "@angular-eslint/eslint-plugin-template": "15.2.1",
    "@angular-eslint/template-parser": "15.2.1",
    "@angular/cli": "15.2.4",
    "@angular/compiler-cli": "15.2.2",
    "@angular/localize": "15.2.2",
    "@axe-core/playwright": "4.6.0",
    "@playwright/test": "1.31.2",
    "@types/jasmine": "4.3.1",
    "@types/leaflet": "1.9.3",
    "@typescript-eslint/eslint-plugin": "5.54.1",
    "@typescript-eslint/parser": "5.54.1",
    "concurrently": "7.6.0",
    "eslint": "8.36.0",
    "eslint-config-prettier": "8.7.0",
    "eslint-plugin-prettier": "4.2.1",
    "firebase-tools": "11.24.1",
    "firebase-admin": "11.5.0",
    "jasmine-core": "4.5.0",
    "karma": "6.4.1",
    "karma-chrome-launcher": "3.1.1",
    "karma-coverage": "2.2.0",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.0.0",
    "prettier": "2.8.4",
    "serve": "14.2.0",
    "typescript": "4.9.5",
    "wait-on": "7.0.1"
  }
}
