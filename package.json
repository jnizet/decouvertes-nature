{
  "name": "decouvertes-nature",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "format": "prettier --write \"src/**/*.+(ts|js|json|html|scss)\"",
    "format:check": "prettier --check \"src/**/*.+(ts|js|json|html|scss)\"",
    "serve:e2e": "ng serve -c e2e",
    "e2e": "yarn playwright test",
    "build:e2e:ci": "ng build -c e2e",
    "serve:e2e:ci": "serve dist/decouvertes-nature/browser --single --listen 4201",
    "e2e:ci": "concurrently --names \"emulators,app,test\" --success first --kill-others \"firebase emulators:start --import=emulator-data\" \"yarn build:e2e:ci && yarn serve:e2e:ci\" \"wait-on --timeout 180000 http://localhost:4201 http://localhost:4000 && yarn playwright test\""
  },
  "private": true,
  "engines": {
    "node": ">=16.15.0",
    "yarn": ">=1.22.10 <2.0.0"
  },
  "dependencies": {
    "@angular/animations": "17.3.2",
    "@angular/common": "17.3.2",
    "@angular/compiler": "17.3.2",
    "@angular/core": "17.3.2",
    "@angular/fire": "17.0.1",
    "@angular/forms": "17.3.2",
    "@angular/platform-browser": "17.3.2",
    "@angular/platform-browser-dynamic": "17.3.2",
    "@angular/router": "17.3.2",
    "@ng-bootstrap/ng-bootstrap": "16.0.0",
    "@popperjs/core": "2.11.8",
    "bootstrap": "5.3.3",
    "bootstrap-icons": "1.11.3",
    "date-fns": "3.6.0",
    "firebase": "10.9.0",
    "leaflet": "1.9.4",
    "ngx-valdemort": "11.0.0",
    "rxjs": "7.8.1",
    "tslib": "2.6.2",
    "zone.js": "0.14.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "17.3.2",
    "@angular-eslint/builder": "17.3.0",
    "@angular-eslint/eslint-plugin": "17.3.0",
    "@angular-eslint/eslint-plugin-template": "17.3.0",
    "@angular-eslint/template-parser": "17.3.0",
    "@angular/cli": "17.3.2",
    "@angular/compiler-cli": "17.3.2",
    "@angular/localize": "17.3.2",
    "@axe-core/playwright": "4.8.5",
    "@playwright/test": "1.42.1",
    "@types/jasmine": "5.1.4",
    "@types/leaflet": "1.9.8",
    "@typescript-eslint/eslint-plugin": "7.4.0",
    "@typescript-eslint/parser": "7.4.0",
    "concurrently": "8.2.2",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "firebase-tools": "13.5.2",
    "firebase-admin": "12.0.0",
    "jasmine-core": "5.1.2",
    "karma": "6.4.3",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage": "2.2.1",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.1.0",
    "prettier": "3.2.5",
    "serve": "14.2.1",
    "typescript": "5.4.3",
    "wait-on": "7.2.0"
  },
  "resolutions": {
    "firebase": "10.9.0"
  }
}
