{
  "name": "decouvertes-nature",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "format": "prettier --write \"src/**/*.+(ts|js|json|html|scss)\"",
    "format:check": "prettier --check \"src/**/*.+(ts|js|json|html|scss)\"",
    "postinstall": "node generate-bootstrap-icons.mjs",
    "serve:e2e": "ng serve -c e2e",
    "e2e": "yarn playwright test",
    "build:e2e:ci": "ng build -c e2e",
    "serve:e2e:ci": "serve dist/decouvertes-nature --single --listen 4201",
    "e2e:ci": "concurrently --names \"emulators,app,test\" --success first --kill-others \"firebase emulators:start --import=emulator-data\" \"yarn build:e2e:ci && yarn serve:e2e:ci\" \"wait-on --timeout 180000 http://localhost:4201 http://localhost:4000 && yarn playwright test\""
  },
  "private": true,
  "engines": {
    "node": ">=16.15.0",
    "yarn": ">=1.22.10 <2.0.0"
  },
  "dependencies": {
    "@angular/animations": "16.2.5",
    "@angular/common": "16.2.5",
    "@angular/compiler": "16.2.5",
    "@angular/core": "16.2.5",
    "@angular/fire": "7.6.1",
    "@angular/forms": "16.2.5",
    "@angular/platform-browser": "16.2.5",
    "@angular/platform-browser-dynamic": "16.2.5",
    "@angular/router": "16.2.5",
    "@ng-bootstrap/ng-bootstrap": "15.1.1",
    "@popperjs/core": "2.11.8",
    "bootstrap": "5.3.2",
    "bootstrap-icons": "1.11.0",
    "date-fns": "2.30.0",
    "firebase": "10.4.0",
    "leaflet": "1.9.4",
    "ngx-valdemort": "10.0.1",
    "rxjs": "7.8.1",
    "tslib": "2.6.2",
    "zone.js": "0.13.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "16.2.2",
    "@angular-eslint/builder": "16.1.2",
    "@angular-eslint/eslint-plugin": "16.1.2",
    "@angular-eslint/eslint-plugin-template": "16.1.2",
    "@angular-eslint/template-parser": "16.1.2",
    "@angular/cli": "16.2.2",
    "@angular/compiler-cli": "16.2.5",
    "@angular/localize": "16.2.5",
    "@axe-core/playwright": "4.7.3",
    "@playwright/test": "1.38.0",
    "@types/jasmine": "4.3.5",
    "@types/leaflet": "1.9.4",
    "@typescript-eslint/eslint-plugin": "6.7.0",
    "@typescript-eslint/parser": "6.7.0",
    "concurrently": "8.2.1",
    "eslint": "8.49.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-prettier": "5.0.0",
    "firebase-tools": "12.5.4",
    "firebase-admin": "11.10.1",
    "jasmine-core": "5.1.1",
    "karma": "6.4.2",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage": "2.2.1",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.1.0",
    "prettier": "3.0.3",
    "serve": "14.2.1",
    "typescript": "5.1.6",
    "wait-on": "7.0.1"
  },
  "resolutions": {
    "firebase": "10.4.0"
  }
}
